extends _layout/default

block vars
  -title        = 'Homepage'
  -description  = ''
  -keywords     = ''
  -og_title     = ''
  -url          = ''
  -pageID       = 'homepage'
  -path         = '.'

block scripts

block container
  section.banner
    .banner-img
      img(src=path+"/assets/images/home/banner.jpg", alt="" title="")
  section.h-intro(data-aos='fade-up' data-aos-anchor-placement='top-bottom')
    .container
      .h-intro-wrap
        .h-intro-content.bg-sec
          h2.title.cl-white GIỚI THIỆU VỀ LUẬT MINH TÚ
          p.desc.cl-white Công ty TNHH Luật Minh Tú là công ty tư vấn hàng đầu trong lĩnh vực Mua bán, sáp nhập doanh nghiệp (M&A); Nhượng quyền thương hiệu, Bất động sản và Xây dựng; Phòng Pháp chế doanh nghiệp thuê ngoài và Soạn thảo, rà soát Hợp đồng,
          p.desc.cl-white Được thành lập bởi Thạc sĩ - Luật sư Võ Hồng Tú - Có hơn 13+ năm kinh nghiệm tư vấn pháp lý và các cộng sự, Luật Minh Tú tự hào là công ty được nhiều khách hàng là cá nhân, doanh nghiệp trong và ngoài nước tin tưởng lựa chọn.
          .btn-wrap
            a.btn.btn-trans(href="#" title='' target="_self") Xem thêm
        .h-intro-img
          img(src=path+"/assets/images/home/intro.jpg", alt="" title="")
  section.h-ser(data-aos='fade-up' data-aos-anchor-placement='top-bottom')
    .container
      .wrap
        .heading
          h2.title.cl-pri
            span DỊCH VỤ CHUYÊN BIỆT TẠI&#160;
            span.cl-sec LUẬT MINH TÚ
        .card-list
          -var n=0;
          while n < 6
            a.card(href="#" title='' target="_self")
              .card-img
                img(src=path+"/assets/images/home/card_1.jpg", alt="" title="")
              .card-content
                h3.card-title Tư vấn pháp lý - nhượng quyền thương mại
                p.card-desc Việc ký kết hợp đồng nhượng quyền cần được thực hiện một cách cẩn trọng và chi tiết.
            - n++
